{
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "importOrder": [
    "<BUILTIN_MODULES>",
    "",
    "^react$",
    "^react-dom(/.*)?$",
    "",
    "^[./]+templates(/.*)?$",
    "",
    "^[./]+components(/.*)?$",
    "",
    "^[./]+pages(/.*)?$",
    "",
    "^[./]+models(/.*)?$",
    "",
    "^[./]+contexts(/.*)?$",
    "",
    "^[./]+utils(/.*)?$",
    "",
    "^[./]+adapters(/.*)?$",
    "",
    "^[./]+routers(/.*)?$",
    "",
    "<THIRD_PARTY_MODULES>",
    "",
    "^(?!.*[.]css$)[./].*$",
    "",
    "^.*[.]css$"
  ],
  "importOrderTypeScriptVersion": "5.9.3",
  "importOrderSafeSideEffects": ["^.+[.]css$"],
  "semi": false,
  "arrowParens": "always",
  "endOfLine": "lf",
  "experimentalOperatorPosition": "end",
  "experimentalTernaries": true,
  "htmlWhitespaceSensitivity": "css",
  "printWidth": 120,
  "objectWrap": "preserve",
  "bracketSameLine": false,
  "bracketSpacing": true,
  "embeddedLanguageFormatting": "auto",
  "jsxSingleQuote": true,
  "proseWrap": "always",
  "quoteProps": "as-needed",
  "singleAttributePerLine": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "all",
  "useTabs": false
}
